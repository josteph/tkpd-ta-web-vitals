  "scripts": {
    "start": "node api/index.js",
    "analyze:client": "env-cmd -e production bnr analyze:client",
    "dev:client": "env-cmd -e development bnr dev:client",
    "dev:server": "env-cmd -e development bnr dev:server",
    "build": "bnr deploy",
    "build:static": "bnr deploy:static",
    "build:client": "bnr build:client",
    "build:server": "bnr build:server",
    "setup": "cp .env.example .env && yarn"
  },
  "betterScripts": {
    "deploy": {
      "command": "babel-node scripts/deploy",
      "env": {
        "TARGET_ENV": "production"
      }
    },
    "deploy:static": {
      "command": "babel-node scripts/deploy",
      "env": {
        "TARGET_ENV": "production",
        "STATIC_DEPLOYMENT": true
      }
    },
    "analyze:client": {
      "command": "babel-node --max-old-space-size=4096 scripts/analyze",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:client": {
      "command": "babel-node scripts/client-build.js",
      "env": {
        "DEBUG": "build*",
        "DIR": "client",
        "NODE_ENV": "production"
      }
    },
    "build:server": {
      "command": "babel-node --max-old-space-size=4096 scripts/run build",
      "env": {
        "DEBUG": "build*",
        "DIR": "server",
        "NODE_ENV": "production"
      }
    },
    "dev:server": {
      "command": "webpack --config ./tools/server/webpack.config.babel.js",
      "env": {
        "DEBUG": "app*,build*,middleware:*,redis*",
        "DIR": "server",
        "NODE_ENV": "development"
      }
    },
    "dev:client": {
      "command": "babel-node scripts/start.js",
      "env": {
        "DIR": "client",
        "NODE_ENV": "development"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
